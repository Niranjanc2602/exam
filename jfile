pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'build'
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
            }
        }
        stage('code') {
            steps {
                echo 'code World'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy World'
            }
        }
    }
}
